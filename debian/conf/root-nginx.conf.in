include /%path_mod_enabled%/root-*.conf;

user root root;
pid %root_file_pid%;
lock_file %root_file_lock%;

error_log %root_file_log_error% warn;

include /%path_conf_enabled%/root-core-*.conf;

http {
	access_log %root_file_log_access%;

	client_body_temp_path %root_path_temp_client%;
	fastcgi_temp_path     %root_path_temp_fastcgi%;
	proxy_temp_path       %root_path_temp_proxy%;
	scgi_temp_path        %root_path_temp_scgi%;
	uwsgi_temp_path       %root_path_temp_uwsgi%;

	include /%path_conf_enabled%/root-http-*.conf;
	include /%path_site_enabled%/root-http-*.conf;
}

include /%path_conf_enabled%/root-module-*.conf;
